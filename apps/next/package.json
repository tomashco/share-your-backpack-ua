{
  "name": "next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "TAMAGUI_TARGET=web && yarn with-env next dev",
    "build": "TAMAGUI_TARGET=web && yarn with-env next build",
    "with-env": "dotenv -e ../../.env.local --",
    "clean": "rm -rf .next .turbo node_modules .tamagui",
    "start": "next start",
    "serve": "NODE_ENV=production next start --port 8151",
    "lint": "next lint"
  },
  "dependencies": {
    "@tamagui/config": "latest",
    "@tamagui/next-theme": "latest",
    "app": "0.0.0",
    "dotenv-cli": "^7.3.0",
    "next": "^13.5.2",
    "raf": "^3.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.72.5",
    "react-native-web": "~0.19.6",
    "vercel": "latest"
  },
  "devDependencies": {
    "@tamagui/next-plugin": "latest",
    "@types/node": "^18.6.4",
    "eslint-config-next": "^13.0.4"
  }
}
