{
  "expo": {
    "name": "share-your-backpack",
    "slug": "share-your-backpack",
    "scheme": "share-your-backpack",
    "version": "0.7.0",
    "orientation": "portrait",
    "icon": "./assets/travel-bag.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shareyourbackpack.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/travel-bag.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.shareyourbackpack.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router", "expo-build-properties"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d5fe69ed-1649-48f8-8d30-b526660afaeb"
      }
    }
  }
}
