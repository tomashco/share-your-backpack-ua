{
  "expo": {
    "name": "sharepack",
    "slug": "sharepack",
    "scheme": "sharepack",
    "version": "0.8.0",
    "orientation": "portrait",
    "icon": "./assets/travel-bag.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sharepack.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/travel-bag.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.sharepack.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router", "expo-build-properties"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ba716a2f-f525-41b0-83e3-ca9c7545114c"
      }
    }
  }
}
